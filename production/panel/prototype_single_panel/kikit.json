{
    "layout": {
        "hspace": "2mm",
        "vspace": "2mm",
        "hbackbone": "4mm",
        "vbackbone": "4mm",
        "rows": "3"
    },
    "source": {
        "type": "annotation",
        "tolerance": "15mm",
        "ref": "B1"
    },
    "tabs": {
        "type": "annotation",
        "tabfootprints": "PCM_kikit:Tab",
        "fillet": "0.25mm"
    },
    "cuts": {
        "type": "mousebites"
    },
    "framing": {
        "type": "frame",
        "cuts": "v"
    },
    "text": {
        "type": "simple",
        "voffset": "3mm",
        "text": "{boardTitle} - Rev. {boardRevision} - Main board"
    },
    "text2": {
        "type": "simple",
        "voffset": "-3mm",
        "text": "{boardCompany} - {boardDate}",
        "anchor": "mb"
    },
    "copperfill": {
        "type": "hatched",
        "clearance": "2mm"
    },
    "page": {
        "type": "A3"
    },
    "post": {
        "millradius": "1mm",
        "dimensions": "True"
    }
}